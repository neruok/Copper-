            <ExtXZnYFar2>Textures\Voxels\Copper_01_ForAxisXZ_distance_add.dds</ExtXZnYFar2>

            <Far2Distance>150</Far2Distance>
            <Far2Scale>80</Far2Scale>
            <LandingEffect>Landing_Jet_Ground_Dust</LandingEffect>
        </VoxelMaterial>
