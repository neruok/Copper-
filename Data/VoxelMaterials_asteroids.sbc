            <SimpleTilingSetup>
                <InitialScale>3</InitialScale>
                <ScaleMultiplier>5</ScaleMultiplier>
                <InitialDistance>5</InitialDistance>
                <DistanceMultiplier>4</DistanceMultiplier>
                <Far1Distance>14</Far1Distance>
                <Far1Scale>12</Far1Scale>
                <Far2Distance>150</Far2Distance>
                <Far2Scale>80</Far2Scale>
